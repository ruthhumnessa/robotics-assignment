   <link name='base'>
            <pose>0 0 .05 0 0 0</pose>
            <inertial>
            <mass>50</mass>
            </inertial>

            <collision name='collision'>
              <geometry>
                <cylinder>
                  <radius>0.5</radius>
                  <length>.1</length>
                </cylinder>
              </geometry>
            </collision>

            <visual name='visual'>
              <geometry>
              <cylinder>
               <radius>0.5</radius>
                <length>.1</length>
                </cylinder>
              </geometry>
            </visual>
    </link>

    <joint name="arm1_base_joint" type="revolute">
        <parent>base</parent>
        <child>arm1</child>
        <pose>0 0 -0.025 0 0 0</pose>
        <axis>
          <xyz>0 0 1</xyz>
          <dynamics>
                    <damping>2.0</damping>
                    <friction>0.7</friction>
                </dynamics>
                <limit>
                    <effort>100</effort>
                    <velocity>1.0</velocity>
                </limit>
        </axis>

       <!-- <dynamics damping="0.7" friction="100.0" /> -->
        
    </joint>

